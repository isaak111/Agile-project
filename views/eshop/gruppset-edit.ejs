<%- include("header", {title: title}); %>

<h1>Edit gruppset details</h1>

<% res = res[0] %>

<p>This is details for the gruppset <%= res.gruppset %>.</p>

<pre><%#= JSON.stringify(res, null, 4) %></pre>

<form class="label-left" method="post" action="/eshop/edit">
    <fieldset>
        <legend>Edit Gruppset</legend>

        <label for="id">Id</label>
        <input id="id" type="text" name="id" readonly value="<%= res.id %>">

        <% if (res.gruppset != null) { %>
        <label for="name">Gruppset</label>
        <input id="gruppset" type="text" name="gruppset" value="<%= res.gruppset %> "> <% } %>

        <input type="submit" name="doit" value="Edit">
    </fieldset>
</form>

<%- include("footer"); %>
